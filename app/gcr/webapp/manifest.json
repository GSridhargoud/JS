{
  "_version": "1.76.0",
  "sap.app": {
    "id": "gcr",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.0",
      "toolsId": "82e2b8b2-2c44-42be-9032-d8ac396ac65c"
    },
    "dataSources": {
      "mainService": {
        "uri": "/gr/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "gcr.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "GRHeadersList",
          "target": "GRHeadersList"
        },
        {
          "pattern": "GRHeaders({key}):?query:",
          "name": "GRHeadersObjectPage",
          "target": "GRHeadersObjectPage"
        },
        {
          "pattern": "GRHeaders({key})/to_gr_items({key2}):?query:",
          "name": "GRItemsObjectPage",
          "target": "GRItemsObjectPage"
        }
      ],
      "targets": {
        "GRHeadersList": {
          "type": "Component",
          "id": "GRHeadersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/GRHeaders",
              "variantManagement": "Page",
              "navigation": {
                "GRHeaders": {
                  "detail": {
                    "route": "GRHeadersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "GRHeadersObjectPage": {
          "type": "Component",
          "id": "GRHeadersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/GRHeaders",
              "navigation": {
                "to_gr_items": {
                  "detail": {
                    "route": "GRItemsObjectPage"
                  }
                }
              }
            }
          }
        },
        "GRItemsObjectPage": {
          "type": "Component",
          "id": "GRItemsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/GRHeaders/to_gr_items"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
